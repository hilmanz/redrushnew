		<div id="main-container" class="bg-challenge" style="position:fixed;">
		<div class="wrapper">
			<div id="progress-content">
					<div id="progressbar" class="ui-progressbar ui-widget ui-widget-content ui-corner-all" role="progressbar" aria-valuemin="0" aria-valuemax="100" aria-valuenow="1"></div>
			<div><h1>Please Wait While Loading Images...</h1></div>
			</div>
		</div>
		</div>
		
{literal}
 <script src="https://ajax.googleapis.com/ajax/libs/jqueryui/1.8/jquery-ui.min.js"></script>
<script>
{/literal}
var target = '{$targetIt}';
{literal}
  var millisecondsLoading =0;
  var startTime = (new Date()).getTime();
  var time=0;
  $(window).load(function () {
       var endTime = (new Date()).getTime();
		millisecondsLoading = endTime - startTime;
       // Put millisecondsLoading in a hidden form field
       // or Ajax it back to the server or whatever.
	     $( "#progressbar" ).progressbar({value:1});
		  var progress =  setInterval(function(){ 
		   time++;
		   $("#progressbar .ui-progressbar-value").animate({ width: time+"%"}, {queue: false});
			if(time==100) {
			clearInterval(progress);
			window.location= target;
		  }
		  }, millisecondsLoading/1000); 
	});


</script>
{/literal}
