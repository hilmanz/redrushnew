/*DatePicker*/
.selectdate .ui-daterangepicker-arrows {
    background: url("../images/tr_black.png") repeat scroll 0 0 transparent;
    border: medium none;
    border-radius: 5px 5px 5px 5px;
    float: left;
    font-size: 12px;
    height: 20px;
    padding: 5px 10px;
}
#content .ui-daterangepicker-prev {
    background: url("../images/arrow_left.png") no-repeat scroll 0 0 transparent;
    height: 19px;
    left: 5px;
    top: 6px;
    width: 19px;
}
.selectdate .ui-widget-header .ui-icon {
    background: none repeat scroll 0 0 transparent;
}
#content .ui-daterangepicker-next {
    background: url("../images/arrow_right.png") no-repeat scroll 0 0 transparent;
    height: 19px;
    right: 5px;
    top: 6px;
    width: 19px;
}
.ui-daterangepicker-prev:hover,#content .ui-daterangepicker-next:hover{
	border:none;
}
.selectdate label {
    display: block;
    float: left;
    line-height: 30px;
    padding: 0 0 0 2px;
    width: 55px;
}
