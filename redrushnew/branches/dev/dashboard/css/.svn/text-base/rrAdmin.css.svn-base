	-moz-border-radius:4px;
	-webkit-border-radius:4px;
	border-radius:4px;
}
#glossary{
	font-size:12px;
	display:table;
	padding:0 0 100px 0;
}
#glossary .title{
	 background: url(../images/border.png) bottom repeat-x;
	 padding:15px 0;
	 clear:both;
}
