}
#login input[type="text"]{
	background: url("../images/input_usr.png") no-repeat scroll 0 0 transparent;
    border: medium none;
    color: #666666;
    font-size: 10px;
    height: 13px;
    padding: 7px 8px;
    width: 193px;
    top:50px;
    left: 37px
}
#login input[type="password"]{
	background: url("../images/input_pass.png") no-repeat scroll 0 0 transparent;
