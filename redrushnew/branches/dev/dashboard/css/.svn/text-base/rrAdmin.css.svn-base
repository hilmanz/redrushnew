.bgChart{
	border:solid 1px #000000;
background-image: linear-gradient(bottom, rgb(0,0,0) 9%, rgb(79,79,79) 64%, rgb(117,117,117) 92%);
background-image: -o-linear-gradient(bottom, rgb(0,0,0) 9%, rgb(79,79,79) 64%, rgb(117,117,117) 92%);
background-image: -moz-linear-gradient(bottom, rgb(0,0,0) 9%, rgb(79,79,79) 64%, rgb(117,117,117) 92%);
background-image: -webkit-linear-gradient(bottom, rgb(0,0,0) 9%, rgb(79,79,79) 64%, rgb(117,117,117) 92%);
background-image: -ms-linear-gradient(bottom, rgb(0,0,0) 9%, rgb(79,79,79) 64%, rgb(117,117,117) 92%);

background-image: -webkit-gradient(
	linear,
	left bottom,
	left top,
	color-stop(0.09, rgb(0,0,0)),
	color-stop(0.64, rgb(79,79,79)),
	color-stop(0.92, rgb(117,117,117))
);
	-moz-border-radius:3px;
	-webkit-border-radius:3px;
	border-radius:3px;
	-moz-box-shadow:0 0 5px #000, 0 0 4px #666 inset;
	-webkit-box-shadow:0 0 5px #000, 0 0 4px #666 inset;
	box-shadow:0 0 5px #000, 0 0 4px #666 inset;
	display:table;    
	padding: 25px;
    width: 944px;
	margin:20px 0 10px 0;
}
.openSesame{
	display:block;
	clear:both;
}
.openSesame button{
	background:url(../images/arrowDown.png) right no-repeat;
	text-align:left;
	color:#fff;
	font-size:14px;
	cursor:pointer;
	height:40px;
	line-height:40px;
	width:100%;
	border:none;
}
.show button{
	background:url(../images/arrowUp.png) right no-repeat;
}
.theRow{
	margin:0;
	padding:10px 0;
}
.theRow .big-chart{
	text-align:center;
}
.bgBlack{
