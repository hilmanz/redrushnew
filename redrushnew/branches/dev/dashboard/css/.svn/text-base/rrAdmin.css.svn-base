	font-weight:normal;
}
#glossary .row{
	 background: url(../images/border.png) bottom repeat-x;
	 clear:both;
	 display:table;
	 width:100%;
	 padding:15px 0;
}
.boxRight .entry{
	text-transform:uppercase;
