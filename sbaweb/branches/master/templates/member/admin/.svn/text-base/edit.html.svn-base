<div style="padding:10px;"><p><h2>Member Management</h2></p><br />{if $msg}<p><div style="color:#cc0000;"><h3>{$msg}</h3></div></p>{/if}<form><input type="hidden" name="s" value="member" /><input type="hidden" name="act" value="edit" /><input type="hidden" name="edit" value="1" /><input type="hidden" name="id" value="{$id}" /><input type="hidden" name="regid" value="{$list.id}" /><table width="100%" border="0" cellspacing="0" cellpadding="0" class="addlist zebra">  <tr>	<td colspan="2"><b>Edit Member</b></td>  </tr>	<tr>		<td>Username</td><td><input type="text" name="username" value="{$list.username}" disabled="disabled" /></td>	</tr>	<tr>		<td>Nama</td><td><input type="text" name="nama" value="{$list.nama}" /></td>	</tr>	<tr>		<td>Panggilan</td><td><input type="text" name="panggilan" value="{$list.panggilan}" /></td>	</tr>	<tr>		<td>Jenis Kelamin</td>		<td>			<select name="jenis_kelamin">				<option value="L" {if $list.jenis_kelamin eq 'L'}selected="selected"{/if}>Laki-laki</option>				<option value="P" {if $list.jenis_kelamin eq 'P'}selected="selected"{/if}>Perempuan</option>			</select>	</tr>	<tr>		<td>Tempat Lahir</td><td><input type="text" name="tempat_lahir" value="{$list.tempat_lahir}" /></td>	</tr>	<tr>		<td>Tanggal Lahir</td><td><input type="text" name="tanggal_lahir" value="{$list.tgl_lahir}" /></td>	</tr>	<tr>		<td>No. ID</td><td><input type="text" name="no_id" value="{$list.no_id}" /></td>	</tr>	<tr>		<td>Email</td><td><input type="text" name="email" value="{$list.email}" /></td>	</tr>	<tr>		<td>Kota</td><td><input type="text" name="kota" value="{$list.kota}" /></td>	</tr>	<tr>		<td>Propinsi</td><td><input type="text" name="propinsi" value="{$list.provinsi}" /></td>	</tr>	<tr>		<td>Merk Rokok 1</td><td><input type="text" name="merk_rokok_1" value="{$list.merk_rokok_1}" /></td>	</tr>	<tr>		<td>Merk Rokok 2</td><td><input type="text" name="merk_rokok_2" value="{$list.merk_rokok_2}" /></td>	</tr>	<tr>		<td>Acc Social Media</td><td><input type="text" name="acc_social_media" value="{$list.acc_social_media}" /></td>	</tr>	<tr>		<td>Password</td><td><input type="text" name="password" value="" /></td>	</tr>	<tr>		<td>Type</td>		<td>			<select name="type">				<option value="0" {if $list.type eq '0'}selected="selected"{/if}>Entourage</option>				<option value="1" {if $list.type eq '1'}selected="selected"{/if}>BA</option>				<option value="2" {if $list.type eq '2'}selected="selected"{/if}>Management</option>			</select>		</td>	</tr>	<tr>		<td>Status</td>		<td>			<select name="status">				<option value="0" {if $list.n_status eq '0'}selected="selected"{/if}>Block</option>				<option value="1" {if $list.n_status eq '1'}selected="selected"{/if}>Approve</option>			</select>		</td>	</tr>	<tr>		<td colspan="2"><input type="submit" value=" Edit Member " /></td>	</tr></table></form></div>