<div style="padding:10px;"><p><h2>Member Management</h2></p><br /><table width="100%" border="0" cellspacing="0" cellpadding="0" class="addlist zebra">  <tr>	<td colspan="5">		<form>		<input type="hidden" name="s" value="member" />		<input type="hidden" name="act" value="add" />		<input type="submit" value="Add member" />		</form>	</td>  </tr>  <tr class="head">    <td><strong>Username</strong></td>	<td><strong>Name</strong></td>	<td><strong>Email</strong></td>	<td><strong>Type</strong></td>    <td><strong>&nbsp;</strong></td>  </tr>  {section name=i loop=$list}  <tr>    <td>{$list[i].username}</td>    <td>{$list[i].name}</td>    <td>{$list[i].email|stripslashes|strip_tags}</td>    <td>{$list[i].type|stripslashes|strip_tags}</td>	<td><a href="index.php?s=member&act=edit&id={$list[i].id}">Edit</a></td>  </tr>  {/section}</table><p>{$paging}</p></div>