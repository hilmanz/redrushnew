<p><h3>{$title} Activity Per Day</h3></p><br /><table class="addlist zebra" cellspacing="0" cellpadding="0" border="0" width="100%">	<tr>		<td colspan="{if $act eq 4 or $act eq 5}4{else}3{/if}" align="right">			<b>Total: {$total|intval}</b>&nbsp;&nbsp;			<input type="button" value="Export to excel" onclick="javascript:exportToExcel();" />		</td>	</tr>	<tr class="head oddRow">		<td>{if $act eq 4 or $act eq 5}Date{else}Date{/if}</td>		<td>{if $act eq 4 or $act eq 5}User ID{else}Name{/if}</td>		<td>{if $act eq 4 or $act eq 5}Name{else}Email{/if}</td>		{if $act eq 4 or $act eq 5}<td>Email</td>{/if}	</tr>	{section name=i loop=$list}	<tr>		<td>{$list[i].tgl|date_format}</td>		<td>{if $act eq 4 or $act eq 5}{$list[i].user_id|stripslashes}{else}{$list[i].name|stripslashes}{/if}</td>		<td>{if $act eq 4 or $act eq 5}{$list[i].name|stripslashes}{else}{$list[i].email|stripslashes}{/if}</td>		{if $act eq 4 or $act eq 5}<td>{$list[i].email|stripslashes}</td>{/if}	</tr>	{/section}</table>{literal}<script type="text/javascript">function exportToExcel(){	document.location.href = "index.php?s=report&act={/literal}{$act}{literal}&export=1";}</script>{/literal}